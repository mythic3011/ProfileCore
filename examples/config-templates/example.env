# ProfileCore Environment Variables
# Copy this to ~/.config/shell-profile/.env and customize

# =============================================================================
# Git & GitHub
# =============================================================================
export GITHUB_TOKEN="ghp_your_github_token_here"
export GITHUB_USER="your-github-username"

# =============================================================================
# Cloud Providers
# =============================================================================
# AWS
export AWS_ACCESS_KEY_ID="your_aws_key_here"
export AWS_SECRET_ACCESS_KEY="your_aws_secret_here"
export AWS_DEFAULT_REGION="us-east-1"

# Azure
export AZURE_SUBSCRIPTION_ID="your_subscription_id"

# Google Cloud
export GOOGLE_CLOUD_PROJECT="your-project-id"

# =============================================================================
# AI & APIs
# =============================================================================
# OpenAI (for future AI features)
export OPENAI_API_KEY="sk-your_openai_key_here"

# Other APIs
export API_KEY="your_api_key_here"

# =============================================================================
# Development
# =============================================================================
# Node.js
export NODE_ENV="development"

# Python
export PYTHONPATH="/path/to/your/python/modules"

# Custom project paths
export MY_PROJECT_PATH="/path/to/project"
export MY_DATA_DIR="/path/to/data"

# =============================================================================
# Personal
# =============================================================================
export MY_EMAIL="your@email.com"
export MY_NAME="Your Name"

# =============================================================================
# ProfileCore Settings
# =============================================================================
# Enable performance monitoring
export PROFILECORE_MONITOR="0"  # Set to 1 to enable

# Custom cache TTL (seconds)
export PROFILECORE_CACHE_TTL="300"

# =============================================================================
# Notes
# =============================================================================
# - Never commit this file to version control
# - Keep sensitive data secure
# - Use different .env files for work/personal
# - Reload profile after changes: . $PROFILE


